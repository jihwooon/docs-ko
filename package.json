{
  "name": "@vitest/monorepo",
  "type": "module",
  "version": "1.4.0",
  "private": true,
  "packageManager": "pnpm@8.10.3",
  "description": "Next generation testing framework powered by Vite",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "dev": "vitepress dev docs ",
    "build": "nr prefetch && vitepress build docs",
    "build-no-prefetch": "vitepress build docs",
    "serve": "vitepress serve docs",
    "preview-https": "pnpm run build docs && serve .vitepress/dist",
    "preview-https-no-prefetch": "pnpm run build-no-prefetch && serve .vitepress/dist",
    "prefetch": "tsx .vitepress/scripts/fetch-avatars.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "generate-pwa-icons": "pwa-assets-generator",
    "docs:examples": "tsx scripts/update-examples.ts",
    "docs:contributors": "tsx scripts/update-contributors.ts"
  },
  "dependencies": {
    "@vueuse/core": "^10.6.1",
    "vue": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@antfu/ni": "^0.18.8",
    "@iconify-json/carbon": "^1.1.28",
    "@iconify-json/logos": "^1.1.42",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.18",
    "@unocss/reset": "^0.58.5",
    "@vite-pwa/assets-generator": "^0.0.11",
    "@vite-pwa/vitepress": "^0.2.3",
    "@vitejs/plugin-vue": "latest",
    "eslint": "^8.34.0",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.1.1",
    "https-localhost": "^4.7.1",
    "ofetch": "^1.3.3",
    "ohmyfetch": "^0.4.21",
    "pathe": "^0.3.9",
    "pnpm": "^8.15.4",
    "tsx": "^4.1.1",
    "typescript": "5.0.4",
    "unocss": "^0.58.5",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.16.7",
    "vitepress": "^1.0.0-rc.35",
    "workbox-window": "^7.0.0"
  }
}
